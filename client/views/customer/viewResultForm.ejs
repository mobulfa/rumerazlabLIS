<% layout('/layouts/boilerplate') %>

    <div class="container">
        <div class="row">
            <div class="col-6 offset-3">
                <input type="text" class="form-control" placeholder="Input Reference No">
                <h3>Result Form</h3>

        <form>
        <div>
            <label for="fname">First Name:</label>
            <input type="text" id="fname" name="fname" placeholder="Input First Name">
        </div>
        <br>
        <div>
            <label for="lname">Last Name:</label>
            <input type="text" id="lname" name="lname" placeholder="Input Last Name">
        </div>
        <br>
        <div>
            <label for="address">Age:</label>
            <input type="text" id="age" name="age" placeholder="Input Age">
        </div>
        <br>
        <div>
            <label for="adress">Address:</label>
            <input type="text" id="adress" name="adress" placeholder="Input Address">
        </div>
        <br>
        <div>
            <label for="pass">Password:</label>
            <input type="password" id="pass" name="pass" maxlength="12" required>
        </div>
        <br>
        <div>
            <label for="pass">Email:</label>
            <input type="email" id="email" name="email" placeholder="Input Email" required>
        </div>

        <div>
            <label for="title"> Title:</label>
            
            <label for="Mr.">Mr.</label>
            <input type="radio" id="Mr." name="title" value="Mr.">

            <label for="Mrs.">Mrs.</label>
            <input type="radio" id="Mrs." name="title" value="Mrs.">

            <label for="Mr.">PhD</label>
            <input type="radio" id="PhD" name="title" value="PhD">
        </div>

        <div>
            <label for="status"></label>
            <select id="status" name="status">
                <option>Patient</option>
                <option>Employee</option>
                <option>Doctor</option>
        </div>

        <div>
            <input type="reset">
        </div>

        <br>
        <div>
            <input type="submit">
        
        </form>

<output form="numform" id="x" name="x" for="a+b"></output>

            </div>
        </div>
    </div>
